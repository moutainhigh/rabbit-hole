<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.lotus.pay.biz.mapper.RefundRecordMapper">
  <select id="pagingByTradeId" resultType="com.github.lotus.pay.biz.entity.RefundRecord">
    SELECT brr.*
    FROM bmw_refund_record brr
    <where>
      brr.trade_id = #{tradeId}
    </where>
  </select>
</mapper>
