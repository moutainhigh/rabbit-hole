<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.lotus.com.biz.mapper.UserIntegralFlowMapper">
  <select id="pageFlow" resultType="com.github.lotus.com.biz.entity.UserIntegralFlow">
    SELECT cuif.*
    FROM com_user_integral_flow cuif
    <where>
      AND cuif.user_id = #{ro.userId}
    </where>
  </select>
</mapper>
